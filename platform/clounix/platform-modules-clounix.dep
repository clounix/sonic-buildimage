
MPATH       := $($(CLOUNIX_COMMON_PLATFORM_MODULE)_SRC_PATH)
DEP_FILES   := $(SONIC_COMMON_FILES_LIST) platform/clounix/platform-modules-clounix.mk platform/clounix/platform-modules-clounix.dep 
DEP_FILES   += $(SONIC_COMMON_BASE_FILES_LIST)
SMDEP_PATHS := $(MPATH) $(MPATH)/common/fpga_utils $(MPATH)/platform-utilities
$(foreach path, $(SMDEP_PATHS), $(eval $(path) :=$(filter-out $(SMDEP_PATHS),$(addprefix $(path)/, $(shell cd $(path) &&  git ls-files | grep -Ev " " )))))

$(CLOUNIX_COMMON_PLATFORM_MODULE)_CACHE_MODE  := none 
$(CLOUNIX_COMMON_PLATFORM_MODULE)_DEP_FLAGS   := $(SONIC_COMMON_FLAGS_LIST)
$(CLOUNIX_COMMON_PLATFORM_MODULE)_DEP_FILES   := $(DEP_FILES)
$(CLOUNIX_COMMON_PLATFORM_MODULE)_SMDEP_FILES := $(foreach path, $(SMDEP_PATHS), $($(path)))
$(CLOUNIX_COMMON_PLATFORM_MODULE)_SMDEP_PATHS := $(SMDEP_PATHS)

