[Unit]
Description=Flines common Platform initialization service
Before=network.target determine-reboot-cause.service
DefaultDependencies=no

[Service]
Type=simple
ExecStart=/usr/local/bin/dsProcess/main.py install
ExecStop=/usr/local/bin/dsProcess/main.py uninstall
RemainAfterExit=yes

[Install]
WantedBy=multi-user.target
